{
    "manifest_version": 3,
    "name": "Hello world Extension",
    "version": "1.0",
    "description": "Extension that displays hello world.",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png" 
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
    }
  }